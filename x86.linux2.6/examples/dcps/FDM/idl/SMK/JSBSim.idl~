module JSBSim
{
   struct fdmdata
   {
      	long    ID;    // User ID
	double Velocity_Kts;
	double MachNumber;
	double GValue;
	double SideSlip;
	double Roll_deg;
	double Pitch_deg;
	double AtmosphereTemperature;
	double Heading_deg;
	double Altitude_ft;
	double VerticleVelocity_fpm;
      	double Latitude;
	double Longitude;
	double AltitudeAboveSeaLevel_ft;
	double SimTime;
	double AngleOFAttack;
	double TrueAirSpeed;
	double GroundAirSpeed;
		
		      
   };            
   #pragma keylist fdmdata ID
	
};
